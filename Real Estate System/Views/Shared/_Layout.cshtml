                            @if (User.IsInRole("Admin") || User.IsInRole("Sales") || User.IsInRole("salesMember") || User.IsInRole("Excutive"))
                            {
                                <li class="active">
                                    <a asp-controller="Dublex">Apartments<span class="submenu-indicator"></span></a>
                                </li>
                            }
                            @if (User.IsInRole("Admin") || User.IsInRole("Sales") || User.IsInRole("salesMember"))
                            {
                                <li>
                                    <a asp-controller="Sales">Sales<span class="submenu-indicator"></span></a>

                                </li>
                            }

                            @if (User.IsInRole("Admin") || User.IsInRole("Engineer") || User.IsInRole("engineerMember"))
                            {
                                <li>
                                    <a asp-controller="Engineer">Engineers<span class="submenu-indicator"></span></a>

                                </li>
                            }
                            @if (User.IsInRole("Admin") || User.IsInRole("Workers"))
                            {
                                <li>
                                    <a asp-controller="Employessandworkers">Workers<span class="submenu-indicator"></span></a>

                                </li>
                            }
                            @if (User.IsInRole("Admin") || User.IsInRole("Sales") || User.IsInRole("salesMember") || User.IsInRole("CallCenter") || User.IsInRole("callcenterMember"))
                            {

                                <li>
                                    <a asp-controller="Client">Clients<span class="submenu-indicator"></span></a>

                                </li>
                            }
                            @if (User.IsInRole("Admin") || User.IsInRole("CallCenter") || User.IsInRole("callcenterMember"))
                            {
                                <li>
                                    <a asp-controller="CallCenter">CallCenter<span class="submenu-indicator"></span></a>
                                </li>
                            }
                            @if (User.IsInRole("Admin"))
                            {
                                <li>
                                    <a asp-controller="Users" asp-action="Index">Users<span class="submenu-indicator"></span></a>
                                </li>
                            }
                            @if (User.IsInRole("Admin") || User.IsInRole("Sales") || User.IsInRole("Engineer") || User.IsInRole("CallCenter"))
                            {

                                <li>
                                    <a asp-area="Identity" asp-page="/Account/Register">Add New Member<span class="submenu-indicator"></span></a>

                                </li>
                            }
                            <li>
                                <a asp-controller="Home" asp-action="Index">Features<span class="submenu-indicator"></span></a>

                            </li>
                            <li>
                                <a asp-controller="Home" asp-action="About">About Us<span class="submenu-indicator"></span></a>

                            </li>
                            <li>
                                <a asp-controller="Home" asp-action="Contact">Contact<span class="submenu-indicator"></span></a>

                            </li>
                        </ul>

                        <ul class="nav-menu nav-menu-social align-to-right">
                            <li><partial name="_LoginPartial" /></li>
                        </ul>
                    </div>
                </nav>
            </div>
