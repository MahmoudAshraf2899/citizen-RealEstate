@model Real_Estate_System.Models.ApplicationUser
    <div style="margin-top:30px;">
        <h3>Are you sure you want to delete  @Html.DisplayFor(model => model.UserName)</h3>
        <div style="font-weight:bold;">
            <hr />
            <dl class="row">
                <!--UserId-->
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Id)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Id)
                </dd>
                <!--UserId-->
                <!--UserName-->
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.UserName)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.UserName)
                </dd>
                <!--UserName-->
                <!--User Email-->
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Email)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Email)
                </dd>
                <!--UserEmail-->
                <!--User PhoneNumber-->
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.PhoneNumber)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.PhoneNumber)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.PhoneNumber2)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.PhoneNumber2)
                </dd>
                <!--User PhoneNumber-->
            </dl>
            <form asp-action="Delete" style="margin-top:20px;">
                <input type="hidden" asp-for="Id" />
                <input type="submit" value="Delete" class="btn btn-danger" />
                <a asp-action="Index" class="btn btn-info">Back to List</a>
            </form>
        </div>
    </div>

